<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jibushengdan.android.jbsdblog.activity.MainActivity">
    <com.jibushengdan.android.jbsdblog.widget.PathView
        android:id="@+id/long_svg"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:pathColor="@color/colorFont"
        app:svg="@raw/logo_long"
        android:transitionName="long_svg"/>
</RelativeLayout>
